{
  "expo": {
    "name": "ML Express",
    "slug": "market-link-express",
    "version": "1.0.0",
    "scheme": "mlexpress",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mlexpress.app"
    },
    "updates": {
      "url": "https://u.expo.dev/fcbcd7ae-4778-4052-9903-183ef023e8df"
    },
    "runtimeVersion": "1.0.0",
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "extra": {
      "eas": {
        "projectId": "fcbcd7ae-4778-4052-9903-183ef023e8df"
      }
    },
    "plugins": [
      "expo-barcode-scanner",
      "expo-secure-store"
    ]
  }
}
