        "UIBackgroundModes": [
          "location",
          "location"
        ]
      },
      "config": {
        "googleMapsApiKey": "AIzaSyCtf57YS_4-7meheIlUONuf0IPHYDcgilM"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a365d"
      },
      "config": {
